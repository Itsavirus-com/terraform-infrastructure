# Copy this file to secrets.tfvars and fill in the actual values
# DO NOT commit secrets.tfvars to version control

# Replace these with actual AWS Account ID
env_s3_arn             = "arn:aws:s3:::mobypark-prod-config"
parking_data_s3_arn    = "arn:aws:s3:::mobypark-prod-parking-data"
media_storage_s3_arn   = "arn:aws:s3:::mobypark-prod-media"
backup_storage_s3_arn  = "arn:aws:s3:::mobypark-prod-backups"

# Replace with actual account ID and secret names
db_secret_arn  = "arn:aws:secretsmanager:eu-west-1:YOUR_ACCOUNT_ID:secret:mobypark-prod-db-credentials-XXXXXX"
jwt_secret_arn = "arn:aws:secretsmanager:eu-west-1:YOUR_ACCOUNT_ID:secret:mobypark-prod-jwt-secret-XXXXXX"

# Replace with actual SQS queue ARNs
notification_queue_arn = "arn:aws:sqs:eu-west-1:YOUR_ACCOUNT_ID:mobypark-prod-notifications"
payment_queue_arn      = "arn:aws:sqs:eu-west-1:YOUR_ACCOUNT_ID:mobypark-prod-payments"
booking_queue_arn      = "arn:aws:sqs:eu-west-1:YOUR_ACCOUNT_ID:mobypark-prod-bookings"

# Replace with actual DynamoDB table ARNs
session_table_arn          = "arn:aws:dynamodb:eu-west-1:YOUR_ACCOUNT_ID:table/mobypark-prod-sessions"
user_preferences_table_arn = "arn:aws:dynamodb:eu-west-1:YOUR_ACCOUNT_ID:table/mobypark-prod-user-preferences"

# Replace with actual SNS topic ARN
notification_topic_arn = "arn:aws:sns:eu-west-1:YOUR_ACCOUNT_ID:mobypark-prod-notifications" 